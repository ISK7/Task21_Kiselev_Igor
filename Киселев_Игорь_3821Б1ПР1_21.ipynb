   ],
   "source": [
    "m = np.arange(1, a + 1, dtype=float)\n",
    "E = []\n",
    "begin = ti.time()*1000\n",
    "for i in range(0,a):\n",
    "    E.append((m[i]**-1)*mp.besselj(m[i], m[i]*e)*np.sin(m[i]*M))\n",
    "Es = M + 2*sum(E)\n",
    "end = ti.time()*1000\n"
   ]
