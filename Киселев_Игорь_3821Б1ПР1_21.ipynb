    "def HelpFun(n,m,y):\n",
    "    ans = 1.0\n",
    "    for i in range(1,n+1):\n",
    "        ans *= y / i**2\n",
    "    for i in range(1,m+1):\n",
    "        ans *= y/(i+n)\n",
    "    for i in range(1,n+1):\n",
    "        ans *= y\n",
    "    return ans\n",
    "\n",
    "def J2(j,x):\n",
    "    j = int(j)\n",
    "    ansar = []\n",
    "    for i in range(0,a):\n",
    "         ansar.append(((-1)**i)*HelpFun(i,j,x/2))\n",
    "    return sum(ansar)\n",
    "\n",
    "E = []\n",
    "begin = ti.time()*1000\n",
    "for i in range(0,a):\n",
    "    E.append((m[i]**-1)*J2(m[i], m[i]*e)*np.sin(m[i]*M))\n",
    "Ej = M + 2*sum(E)\n",
    "end = ti.time()*1000\n"
   ]
